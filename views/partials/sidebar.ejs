<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="/css/sidebar.css">
</head>


<body >
  <div class="parent something" >
    <div class="userCard">
      <span>
        <img src="/images/user.png" alt="user" class="userImg">
      </span>
      <p class="username"><%=user.local.username%></p>
    </div>
  </div>
  <div id="mySidenav" class="sidenav something" style="padding-top: 100px">
    <% if (user.isSeller==true){%>
        <a href="/sell/" id="ads">My Ads <span><i style="margin-left: 32%;" id="icon"
              class="fa fa-newspaper-o"></i></span></a>
        <a href="/payment/buyad" id="ads">Buy Ads<span><i style="margin-left: 32%;" id="icon"
            class="fas fa-dollar-sign"></i></span></a>
    <%}%>
    <a href="/wish" id="wishlist">My Wishlist <span><i style="margin-left: 10%;" id="icon"
          class="fa fa-heart"></i></span></a>
    <a href="/myprofile/" id="profile">My Profile <span><i style="margin-left: 18%;" id="icon"
          class="fa fa-user"></i></span></a>

  </div>
  <script src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc="
    crossorigin="anonymous"></script>
  <script type="text/javascript" src="/js/sidebar.js">

  </script>
</body>

</html>
